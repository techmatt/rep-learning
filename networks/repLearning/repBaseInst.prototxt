# @relu(name=simulate0-relu1)
layer {
  name: "simulate0-relu1"
  bottom: "simulate0-fc1"
  top: "simulate0-fc1"
  type: "ReLU"
}

# @ip(name=simulate0-fc2,paramName=simFC2Params,outputs=256)
layer {
  name: "simulate0-fc2"
  type: "InnerProduct"
  bottom: "simulate0-fc1"
  top: "simulate0-fc2"
  param { name: "simFC2Paramsw" lr_mult: 1 }
  param { name: "simFC2Paramsb" lr_mult: 2 }
  inner_product_param {
    num_output: 256
    weight_filler { type: "xavier" } bias_filler { type: "constant" }
  }
}

# @relu(name=simulate0-relu2)
layer {
  name: "simulate0-relu2"
  bottom: "simulate0-fc2"
  top: "simulate0-fc2"
  type: "ReLU"
}

# @ip(name=physState1,paramName=simFC3Params,outputs=64)
layer {
  name: "physState1"
  type: "InnerProduct"
  bottom: "simulate0-fc2"
  top: "physState1"
  param { name: "simFC3Paramsw" lr_mult: 1 }
  param { name: "simFC3Paramsb" lr_mult: 2 }
  inner_product_param {
    num_output: 64
    weight_filler { type: "xavier" } bias_filler { type: "constant" }
  }
}

# @relu(name=simulate0-relu3)
layer {
  name: "simulate0-relu3"
  bottom: "physState1"
  top: "physState1"
  type: "ReLU"
}

# @repSimulate(prefix=simulate1,bottom=physState1,top=physState2)

# @ip(name=simulate1-fc0,paramName=simFC0Params,bottom=physState1,outputs=512)
layer {
  name: "simulate1-fc0"
  type: "InnerProduct"
  bottom: "physState1"
  top: "simulate1-fc0"
  param { name: "simFC0Paramsw" lr_mult: 1 }
  param { name: "simFC0Paramsb" lr_mult: 2 }
  inner_product_param {
    num_output: 512
    weight_filler { type: "xavier" } bias_filler { type: "constant" }
  }
}

# @relu(name=simulate1-relu0)
layer {
  name: "simulate1-relu0"
  bottom: "simulate1-fc0"
  top: "simulate1-fc0"
  type: "ReLU"
}

# @ip(name=simulate1-fc1,paramName=simFC1Params,outputs=512)
layer {
  name: "simulate1-fc1"
  type: "InnerProduct"
  bottom: "simulate1-fc0"
  top: "simulate1-fc1"
  param { name: "simFC1Paramsw" lr_mult: 1 }
  param { name: "simFC1Paramsb" lr_mult: 2 }
  inner_product_param {
    num_output: 512
    weight_filler { type: "xavier" } bias_filler { type: "constant" }
  }
}

# @relu(name=simulate1-relu1)
layer {
