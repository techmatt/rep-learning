  bottom: "simulate1-fc1"
  top: "simulate1-fc1"
  type: "ReLU"
}

# @ip(name=simulate1-fc2,paramName=simFC2Params,outputs=256)
layer {
  name: "simulate1-fc2"
  type: "InnerProduct"
  bottom: "simulate1-fc1"
  top: "simulate1-fc2"
  param { name: "simFC2Paramsw" lr_mult: 1 }
  param { name: "simFC2Paramsb" lr_mult: 2 }
  inner_product_param {
    num_output: 256
    weight_filler { type: "xavier" } bias_filler { type: "constant" }
  }
}

# @relu(name=simulate1-relu2)
layer {
  name: "simulate1-relu2"
  bottom: "simulate1-fc2"
  top: "simulate1-fc2"
  type: "ReLU"
}

# @ip(name=physState2,paramName=simFC3Params,outputs=64)
layer {
  name: "physState2"
  type: "InnerProduct"
  bottom: "simulate1-fc2"
  top: "physState2"
  param { name: "simFC3Paramsw" lr_mult: 1 }
  param { name: "simFC3Paramsb" lr_mult: 2 }
  inner_product_param {
    num_output: 64
    weight_filler { type: "xavier" } bias_filler { type: "constant" }
  }
}

# @relu(name=simulate1-relu3)
layer {
  name: "simulate1-relu3"
  bottom: "physState2"
  top: "physState2"
  type: "ReLU"
}

# @repSimulate(prefix=simulate2,bottom=physState2,top=physState3)

# @ip(name=simulate2-fc0,paramName=simFC0Params,bottom=physState2,outputs=512)
layer {
  name: "simulate2-fc0"
  type: "InnerProduct"
  bottom: "physState2"
  top: "simulate2-fc0"
  param { name: "simFC0Paramsw" lr_mult: 1 }
  param { name: "simFC0Paramsb" lr_mult: 2 }
  inner_product_param {
    num_output: 512
    weight_filler { type: "xavier" } bias_filler { type: "constant" }
  }
}

# @relu(name=simulate2-relu0)
layer {
  name: "simulate2-relu0"
  bottom: "simulate2-fc0"
  top: "simulate2-fc0"
  type: "ReLU"
}

# @ip(name=simulate2-fc1,paramName=simFC1Params,outputs=512)
layer {
  name: "simulate2-fc1"
  type: "InnerProduct"
  bottom: "simulate2-fc0"
  top: "simulate2-fc1"
  param { name: "simFC1Paramsw" lr_mult: 1 }
  param { name: "simFC1Paramsb" lr_mult: 2 }
