  name: "decode2-relu3"
  bottom: "decode2-fc3"
  top: "decode2-fc3"
  type: "ReLU"
}

# @reshape(name=decode2-reshape,dimA=64,dimB=7,dimC=7)
layer {
name: "decode2-reshape"
